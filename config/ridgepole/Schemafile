# カード
create_table "cards", force: :cascade do |t|
  t.string   "name",             comment:""
  t.string   "multiverseid",             comment:""
  t.string   "layout",             comment:""
  t.string   "names",             comment:""
  t.string   "manaCost",             comment:""
  t.string   "cmc",             comment:""
  t.string   "type",             comment:""
  t.string   "supertypes",             comment:""
  t.string   "subtypes",             comment:""
  t.string   "rarity",             comment:""
  t.string   "text",             comment:""
  t.string   "flavor",             comment:""
  t.string   "artist",             comment:""
  t.string   "number",             comment:""
  t.string   "power",             comment:""
  t.string   "toughness",             comment:""
  t.string   "loyalty",             comment:""
  t.string   "variations",             comment:""
  t.string   "watermark",             comment:""
  t.string   "border",             comment:""
  t.string   "timeshifted",             comment:""
  t.string   "hand",             comment:""
  t.string   "life",             comment:""
  t.string   "reserved",             comment:""
  t.string   "releaseDate",             comment:""
  t.string   "starter",             comment:""
  t.string   "rulings",             comment:""
  t.string   "foreignNames",             comment:""
  t.string   "printings",             comment:""
  t.string   "originalText",             comment:""
  t.string   "originalType",             comment:""
  t.string   "legalities",             comment:""
  t.string   "source",             comment:""
  t.string   "imageUrl",             comment:""
  t.string   "set",             comment:""
  t.string   "setName",             comment:""
  t.string   "cardId",             comment:"もとはid表記"

  t.datetime "deleted",                         comment:"削除フラグ"
  t.datetime "created",                         comment:"登録日時"
  t.datetime "modified",                        comment:"更新日時"
end

# セット
create_table "sets", force: :cascade do |t|
  t.string   "code",                             comment:""
  t.string   "name",                             comment:""
  t.string   "gatherCode",                             comment:""
  t.string   "oldCode",                             comment:""
  t.string   "magicCardsInfoCode",                    comment:""
  t.string   "releaseDate",                             comment:""
  t.string   "border",                             comment:""
  t.string   "type",                             comment:""
  t.string   "block",                             comment:""
  t.string   "onlineOnly",                             comment:""
  t.string   "booster",                             comment:""
  t.string   "mkmId",                             comment:""
  t.string   "mkmName",                             comment:""

  t.datetime "deleted",                         comment:"削除フラグ"
  t.datetime "created",                         comment:"登録日時"
  t.datetime "modified",                        comment:"更新日時"
end

# デッキ
create_table "decks", force: :cascade do |t|
  t.string   "name",                             comment:"名称"
  t.integer  "user_id",                     comment:"デッキid"
  t.string   "memo",                        comment:"メモ"


  t.datetime "deleted",                         comment:"削除フラグ"
  t.datetime "created",                         comment:"登録日時"
  t.datetime "modified",                        comment:"更新日時"
end

# デッキ-カード
create_table "deck_cards", force: :cascade do |t|
  t.integer  "deck_id",                     comment:"デッキid"
  t.integer  "card_id",                     comment:"カードid"
  t.string   "memo",                        comment:"メモ"

  t.datetime "deleted",                         comment:"削除フラグ"
  t.datetime "created",                         comment:"登録日時"
  t.datetime "modified",                        comment:"更新日時"
end

# ユーザー
create_table "users", force: :cascade do |t|
  t.string   "name",                             comment:"名称"

  t.datetime "deleted",                         comment:"削除フラグ"
  t.datetime "created",                         comment:"登録日時"
  t.datetime "modified",                        comment:"更新日時"
end