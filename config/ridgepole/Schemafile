# カード
create_table "cards", force: :cascade do |t|
  t.string   "name",             comment:""
  t.string   "name2",             comment:"両面カード裏面"
  t.string   "manaCost",             comment:""
  t.string   "cmc",             comment:"コスト"
  t.string   "color1",             comment:""
  t.string   "color2",             comment:""
  t.string   "color3",             comment:""
  t.string   "color4",             comment:""
  t.string   "color5",             comment:""
  t.string   "color6",             comment:""
  t.string   "colorIdentity1",             comment:""
  t.string   "colorIdentity2",             comment:""
  t.string   "colorIdentity3",             comment:""
  t.string   "colorIdentity4",             comment:""
  t.string   "colorIdentity5",             comment:""
  t.string   "colorIdentity6",             comment:""
  t.string   "type",             comment:""
  t.string   "supertypes",             comment:"複数ありえる？"
  t.string   "subtype1",             comment:""
  t.string   "subtype2",             comment:""
  t.string   "subtype3",             comment:""
  t.string   "subtype4",             comment:""
  t.string   "subtype5",             comment:""
  t.string   "rarity",             comment:""
  t.string   "set",             comment:""
  t.string   "text",             comment:""
  t.string   "artist",             comment:""
  t.string   "number",             comment:""
  t.string   "power",             comment:""
  t.string   "toughness",             comment:""
  t.string   "multiverseid",             comment:""
  t.string   "imageUrl",             comment:""
  t.string   "name_eng",             comment:""
  t.string   "db_id",             comment:""

  t.string   "status",                           comment:"試合ステータス"
  t.integer  "away_team_id",                     comment:"アウェイチームid"

  t.boolean  "deleted",      default: false,    comment:"削除フラグ"
  t.datetime "deleted_date",                    comment:"削除日時"
  t.datetime "created",                         comment:"登録日時"
  t.datetime "modified",                        comment:"更新日時"
end

# セット
create_table "sets", force: :cascade do |t|
  t.string   "name",                             comment:"リーグ名"
  t.string   "code",                             comment:"略称"
  t.string   "year",                             comment:"年"
  t.string   "current_matchday",                 comment:"節数"
  t.string   "number_of_teams",                  comment:"総チーム数"

  t.boolean  "deleted",      default: false,    comment:"削除フラグ"
  t.datetime "deleted_date",                    comment:"削除日時"
  t.datetime "created",                         comment:"登録日時"
  t.datetime "modified",                        comment:"更新日時"
end

# デッキ
create_table "decks", force: :cascade do |t|


  t.boolean  "deleted",      default: false,    comment:"削除フラグ"
  t.datetime "deleted_date",                    comment:"削除日時"
  t.datetime "created",                         comment:"登録日時"
  t.datetime "modified",                        comment:"更新日時"
end

# デッキ-カード
create_table "deck_cards", force: :cascade do |t|
  t.integer  "deck_id",                     comment:"デッキid"
  t.integer  "card_id",                     comment:"カードid"

  t.boolean  "deleted",      default: false,    comment:"削除フラグ"
  t.datetime "deleted_date",                    comment:"削除日時"
  t.datetime "created",                         comment:"登録日時"
  t.datetime "modified",                        comment:"更新日時"
end